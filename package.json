{
	"name": "vsce-show-triggerwords",
	"displayName": "Show Trigger Words",
	"description": "Emphasize trigger words in files",
	"preview": false,
	"version": "0.2.10",
	"publisher": "MPearon",
	"keywords": [
		"mpearon",
		"trigger",
		"words",
		"format",
		"log"
	],
	"author": {
		"name": "Matthew Pearon",
		"url": "https://CL3VER.life/mpearon"
	},
	"homepage": "https://github.com/mpearon/PUB-vsce.show-TriggerWords/blob/master/README.md",
	"icon": "resources/altIcon-256.png",
	"galleryBanner": {
		"color": "#520000",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mpearon/PUB-vsce.show-TriggerWords"
	},
	"license": "MIT",
	"extensionKind": [
		"ui"
	],
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Formatters"
	],
	"files.associations": {
		"*.log": "log"
	},
	"languages": [
		{
			"id": "log",
			"extensions": [
				".log"
			]
		}
	],
	"activationEvents": [
		"onCommand:extension.showTriggerWords",
		"onLanguage:log"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.showTriggerWords",
				"title": "Show Trigger Words"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
